isbn = {1932-6203 (Electronic)$\backslash$r1932-6203 (Linking)},
issn = {1932-6203},
journal = {PLoS ONE},
mendeley-groups = {Cancer findings},
month = {jul},
number = {7},
pages = {e67579},
pmid = {23861774},
title = {{Effect of Acute Exercise on Prostate Cancer Cell Growth}},
url = {http://dx.plos.org/10.1371/journal.pone.0067579},
volume = {8},
