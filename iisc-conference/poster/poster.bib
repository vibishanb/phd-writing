title = {{Mediated coalescence: a possible mechanism for tumor cellular heterogeneity.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26807328 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4697694},
